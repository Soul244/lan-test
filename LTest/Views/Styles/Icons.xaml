<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style x:Key="IconBox" TargetType="Rectangle">
        <Setter Property="Width" Value="16"/>
        <Setter Property="Height" Value="16"/>
        <Setter Property="Fill" Value="{DynamicResource LabelGrey}"/>
    </Style>

    <Style x:Key="IconBoxWithTrigger" TargetType="Rectangle" BasedOn="{StaticResource IconBox}">
        <Style.Triggers>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType=Button,AncestorLevel=1}, Path=IsMouseOver}" Value="True">
                <Setter Property="Fill" Value="Black"/>
            </DataTrigger>
            <DataTrigger Binding="{Binding RelativeSource={RelativeSource Mode=FindAncestor,AncestorType=Button,AncestorLevel=1}, Path=IsFocused}" Value="True">
                <Setter Property="Fill" Value="{DynamicResource MainColor}"/>
                <Setter Property="Margin" Value="-5,0,0,0"/>
            </DataTrigger>
        </Style.Triggers>
    </Style>

    <Style x:Key="IconBoxButton" TargetType="Rectangle" BasedOn="{StaticResource IconBox}">
        <Setter Property="Margin" Value="16,0,0,0"/>
    </Style>

    <!-- Animation Box-->

    <Style x:Key="AnimationBox" TargetType="Rectangle">
        <Setter Property="Fill" Value="{DynamicResource LabelGrey}"/>
        <Setter Property="Width" Value="350"/>
        <Setter Property="Height" Value="350"/>
        <Setter Property="RenderTransformOrigin" Value="0.5,0.5"/>
        <Setter Property="RenderTransform">
            <Setter.Value>
                <TransformGroup>
                    <ScaleTransform/>
                    <SkewTransform/>
                    <RotateTransform/>
                    <TranslateTransform/>
                </TransformGroup>
            </Setter.Value>
        </Setter>
    </Style>
    
